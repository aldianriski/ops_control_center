# API Configuration
# Backend API base URL
VITE_API_URL=http://localhost:8000/api/v1

# WebSocket Configuration
# WebSocket server URL
VITE_WS_URL=ws://localhost:8000/ws

# Application Configuration
# Application mode: development | production
VITE_APP_MODE=development

# Enable mock data mode (use mock API instead of real backend)
# Set to 'false' once backend is ready
VITE_MOCK_API=true

# Feature Flags
# Enable/disable specific features
VITE_FEATURE_AIOPS=true
VITE_FEATURE_RBAC=true
VITE_FEATURE_AUDIT_LOGS=true

# Analytics & Monitoring
# Google Analytics ID (optional)
VITE_GA_ID=

# Sentry DSN for error tracking (optional)
VITE_SENTRY_DSN=

# External Services
# Grafana URL for dashboard integration (optional)
VITE_GRAFANA_URL=https://grafana.example.com

# Jira URL for ticket linking (optional)
VITE_JIRA_URL=https://your-company.atlassian.net
